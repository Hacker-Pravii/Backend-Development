public class Solution {
    public int solve(int[] A, int B) {
        int count=0;//initializing with 0 count
        for(int i=0;i<A.length;i++){//loop
            for(int j=i+1;j<A.length;j++){//nested loop
                if (A[i]+A[j]==B){//comparing
                    count++; ///increament the counter
                }
            }

        }
        return count;
    }
}
